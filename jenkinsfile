
pipeline {
    agent any


    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Build') {
            steps {
                sh 'dotnet build HelloWorldApp.csproj --configuration Release'
            }
        }

        stage('Test') {
            steps {
                sh 'dotnet test HelloWorldApp.sln --logger "trx;LogFileName=./HelloWorldApp.trx"'
            }
        }

        
    
}
